   "source": [
    "# 표본의 통계량(estimator) 추정\n",
    "mu_sample = np.mean(data_norm)\n",
    "std_sample = np.std(data_norm, ddof=1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "표본의 평균 : 9.933587276613965\n",
      "표본의 표준편차 : 1.0156331314446025\n"
     ]
    }
   ],
   "source": [
    "print('표본의 평균 :', mu_sample)\n",
    "print('표본의 표준편차 :', std_sample)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "metadata": {},
   "outputs": [],
   "source": [
    "# 상한, 하한 추정 (95% 구간 z=1.96)\n",
    "L_ = mu_sample - 1.96 * (std_sample/np.sqrt(n))\n",
    "U_ = mu_sample + 1.96 * (std_sample/np.sqrt(n))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "metadata": {},
   "outputs": [],
   "source": [
    "# 시각화를 위한 Sample 정보를 이용한 정규분포 생성\n",
    "new_norm = norm(mu_sample, std_sample)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 20,
   "metadata": {},
   "outputs": [
    {
     "data": {
